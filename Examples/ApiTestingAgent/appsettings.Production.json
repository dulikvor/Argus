{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GitHubAuthenticationClient": {
    "Endpoint": "https://api.github.com"
  },
  "GitHubLLMQueryClient": {
    "Endpoint": "https://api.githubcopilot.com",
    "DefaultModelId": "gpt-4o"
  },
  "GitHubRawContentCdnClient": {
    "Endpoint": "https://raw.githubusercontent.com"
  },
  "AzureLLMQueryClient": {
    "Endpoint": "https://<Your_Account>.openai.azure.com",
    "DefaultModelId": "<Your_Model>",
    "ApiKey": "<Your_Api_Key_Here>"
  },
  "AzureEmbeddingClient": {
    "Endpoint": "https://<Your_Account>.openai.azure.com/openai/deployments/<Model_Id>/",
    "ApiKey": "<Your_Api_Key_Here>"
  },
  "AzureMonitor": {
    "ConnectionString": "<Your_Connection_String_Here>"
  }
}